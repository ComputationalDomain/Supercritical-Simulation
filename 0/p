FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    Inlet
    {
        type            inletOutlet;
        inletValue           uniform 101325;
    }

    Outlet
    {
        type            zeroGradient;
    }
	
	Side
	{
		type			empty;
	}

    Airfoil 
    {
        type            zeroGradient;
    }
}

